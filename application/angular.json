{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "defaultProject": "frontend",
  "newProjectRoot": "projects",
  "projects": {
    "frontend": {
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "configurations": {
            "production": {
              "budgets": [
                {
                  "maximumError": "5mb",
                  "maximumWarning": "2mb",
                  "type": "initial"
                }
              ],
              "buildOptimizer": true,
              "commonChunk": true,
              "extractCss": true,
              "extractLicenses": true,
              "namedChunks": true,
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "statsJson": false,
              "vendorChunk": true,
              "watch": false,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/production.environment.ts"
                }
              ]
            },
            "development": {
              "budgets": [
                {
                  "maximumError": "5mb",
                  "maximumWarning": "2mb",
                  "type": "initial"
                }
              ],
              "buildOptimizer": false,
              "commonChunk": false,
              "extractCss": false,
              "extractLicenses": false,
              "namedChunks": false,
              "optimization": false,
              "outputHashing": "none",
              "sourceMap": true,
              "statsJson": true,
              "vendorChunk": false,
              "watch": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/production.environment.ts"
                }
              ]
            }
          },
          "options": {
            "aot": true,
            "assets": ["src/assets", "src/favicon.ico"],
            "deleteOutputPath": false,
            "es5BrowserSupport": true,
            "forkTypeChecker": true,
            "index": "src/index.html",
            "main": "src/main.ts",
            "outputPath": "./../",
            "polyfills": "src/polyfills.ts",
            "preserveSymlinks": true,
            "progress": true,
            "scripts": [],
            "showCircularDependencies": true,
            "styles": ["src/styles.scss"],
            "subresourceIntegrity": true,
            "tsConfig": "src/tsconfig.app.json"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "frontend:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "configurations": {
            "debugging": {
              "fix": true,
              "silent": true,
              "force": true,
              "format": "prose"
            },
            "inspection": {
              "fix": false,
              "silent": false,
              "force": false,
              "format": "stylish"
            }
          },
          "options": {
            "exclude": ["**/node_modules/**"],
            "tsConfig": ["src/tsconfig.app.json", "src/tsconfig.spec.json"],
            "typeCheck": true
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "frontend:build:production",
              "disableHostCheck": false,
              "liveReload": false,
              "open": false,
              "optimization": true,
              "sourceMap": false,
              "vendorChunk": true,
              "watch": false
            },
            "development": {
              "browserTarget": "frontend:build:development",
              "disableHostCheck": true,
              "liveReload": true,
              "open": true,
              "optimization": false,
              "sourceMap": true,
              "vendorChunk": false,
              "watch": true
            }
          },
          "options": {
            "aot": true,
            "commonChunk": true,
            "progress": true,
            "ssl": false
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "assets": ["src/assets", "src/favicon.ico"],
            "karmaConfig": "src/karma.conf.js",
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "scripts": [],
            "styles": ["src/styles.scss"],
            "tsConfig": "src/tsconfig.spec.json"
          }
        }
      },
      "prefix": "app",
      "projectType": "application",
      "root": "",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "sourceRoot": "src"
    },
    "frontend-e2e": {
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "configurations": {
            "production": {
              "devServerTarget": "frontend:serve:production"
            },
            "development": {
              "devServerTarget": "frontend:serve:development"
            }
          },
          "options": {
            "devServerTarget": "frontend:serve",
            "protractorConfig": "e2e/protractor.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "configurations": {
            "debugging": {
              "fix": true,
              "silent": true,
              "force": true,
              "format": "prose"
            },
            "inspection": {
              "fix": false,
              "silent": false,
              "force": false,
              "format": "stylish"
            }
          },
          "options": {
            "exclude": ["**/node_modules/**"],
            "tsConfig": "e2e/tsconfig.e2e.json",
            "typeCheck": true
          }
        }
      },
      "prefix": "",
      "projectType": "application",
      "root": "e2e/"
    }
  },
  "version": 1
}
