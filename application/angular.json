{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "defaultProject": "application",
  "newProjectRoot": "projects",
  "projects": {
    "application": {
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "configurations": {
            "production": {
              "buildOptimizer": true,
              "commonChunk": true,
              "extractCss": true,
              "extractLicenses": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/current.env.ts",
                  "with": "src/environments/production.env.ts"
                }
              ],
              "namedChunks": false,
              "optimization": true,
              "outputHashing": "all",
              "preserveSymlinks": true,
              "progress": true,
              "sourceMap": false,
              "statsJson": false,
              "subresourceIntegrity": true,
              "vendorChunk": true
            }
          },
          "options": {
            "aot": true,
            "assets": [
              "src/assets",
              "src/favicon.ico",
              { "glob": "**/*", "input": "src/assets/", "output": "/assets/" }
            ],
            "baseHref": "/",
            "budgets": [
              {
                "maximumError": "10kb",
                "maximumWarning": "6kb",
                "type": "anyComponentStyle"
              },
              {
                "maximumError": "5mb",
                "maximumWarning": "2mb",
                "type": "initial"
              }
            ],
            "buildOptimizer": false,
            "commonChunk": false,
            "crossOrigin": "none",
            "deleteOutputPath": true,
            "extractCss": false,
            "extractLicenses": false,
            "forkTypeChecker": true,
            "index": "src/index.html",
            "main": "src/main.ts",
            "namedChunks": true,
            "optimization": false,
            "outputHashing": "none",
            "outputPath": "dist/application",
            "polyfills": "src/polyfills.ts",
            "preserveSymlinks": false,
            "progress": true,
            "scripts": [],
            "serviceWorker": false,
            "sourceMap": true,
            "statsJson": true,
            "styles": ["src/styles.scss"],
            "subresourceIntegrity": false,
            "tsConfig": "tsconfig.app.json",
            "vendorChunk": false
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "application:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "configurations": {
            "debugging": {
              "fix": true,
              "force": true,
              "format": "prose",
              "silent": true
            },
            "inspection": {
              "fix": false,
              "force": false,
              "format": "stylish",
              "silent": false
            }
          },
          "options": {
            "exclude": ["**/node_modules/**"],
            "tsConfig": ["tsconfig.app.json"],
            "typeCheck": true
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "application:build",
            "hmr": true,
            "hmrWarning": true,
            "host": "0.0.0.0",
            "liveReload": true,
            "open": true,
            "port": 4200,
            "progress": true,
            "ssl": false,
            "watch": true
          }
        }
      },
      "prefix": "app",
      "projectType": "application",
      "root": "",
      "schematics": {
        "@schematics/angular:class": {
          "skipTests": true
        },
        "@schematics/angular:component": {
          "changeDetection": "OnPush",
          "inlineStyle": false,
          "inlineTemplate": false,
          "skipTests": true,
          "style": "scss",
          "viewEncapsulation": "ShadowDom"
        },
        "@schematics/angular:directive": {
          "skipTests": true
        },
        "@schematics/angular:guard": {
          "skipTests": true
        },
        "@schematics/angular:interceptor": {
          "skipTests": true
        },
        "@schematics/angular:module": {
          "skipTests": true
        },
        "@schematics/angular:pipe": {
          "skipTests": true
        },
        "@schematics/angular:service": {
          "skipTests": true
        }
      },
      "sourceRoot": "src"
    }
  },
  "version": 1
}
