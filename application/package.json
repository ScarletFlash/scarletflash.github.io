{
  "name": "application",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "npm run lint:inspect",
    "lint:inspect": "ng lint --configuration=inspection",
    "lint:debug": "ng lint --configuration=debugging",
    "formatter:check": "prettier --config ./.prettierrc --ignore-path ./.prettierignore --check \"./**/*.{html,css,scss,json,js,ts}\"",
    "formatter:fix": "prettier --loglevel log --config ./.prettierrc --ignore-path ./.prettierignore --write \"./**/*.{html,css,scss,json,js,ts}\"",
    "autofix": "npm run formatter:fix && npm run lint:debug"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.3",
    "@angular/common": "~9.0.3",
    "@angular/compiler": "~9.0.3",
    "@angular/core": "~9.0.3",
    "@angular/forms": "~9.0.3",
    "@angular/platform-browser": "~9.0.3",
    "@angular/platform-browser-dynamic": "~9.0.3",
    "@angular/router": "~9.0.3",
    "gulp-cli": "^2.2.0",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.4",
    "@angular/cli": "~9.0.4",
    "@angular/compiler-cli": "~9.0.3",
    "@angular/language-service": "~9.0.3",
    "@types/node": "^13.7.7",
    "codelyzer": "^5.2.1",
    "gulp": "^4.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "prettier": "^1.19.1",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "<3.8.0"
  }
}
