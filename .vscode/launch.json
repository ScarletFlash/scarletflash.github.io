{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach Chrome",
      "type": "chrome",
      "request": "attach",
      "url": "http://localhost:4200/*",
      "port": 9222,
      "sourceMaps": true,
      "webRoot": "${workspaceRoot}",
      "trace": true,
      "sourceMapPathOverrides": { "webpack:///./*": "${webRoot}/*" }
    }
  ]
}
